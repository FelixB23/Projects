CREATE TABLE client(client_id SMALLINT UNSIGNED NOT NULL AUTO_INCREMENT,nom VARCHAR(30) NOT NULL,prenom VARCHAR(30) NOT NULL,numero_tel VARCHAR(17) NULL,ville VARCHAR(255) NOT NULL, code_postal VARCHAR(6) NOT NULL,PRIMARY KEY (client_id), rue VARCHAR(255) NOT NULL, num_rue VARCHAR(10));
#creation de la table client

CREATE TABLE produits(produit_id SMALLINT UNSIGNED NOT NULL AUTO_INCREMENT,nom VARCHAR(30) NOT NULL,prix DECIMAL(5,2) NOT NULL,type VARCHAR(30) NOT NULL,stock DECIMAL(5,0) NULL,PRIMARY KEY(produit_id));
#creation de la table produits
# cle etrang√®re: CONSTRAINT nom_cle_etrangere FOREIGN KEY(nom_colonne)
#REFERENCES nom_table(nom_colonne_reference)

CREATE TABLE paiement(paiement_id SMALLINT UNSIGNED NOT NULL AUTO_INCREMENT,date_paiement  DATE NOT NULL ,client_id INT UNSIGNED NOT NULL,moyen_paiement VARCHAR(30) NULL,PRIMARY KEY(paiement_id));
#table paiement

